fun int draw(int x){
    int m;
    int d;
    pen(0);
    d = 50;
    m = x;
    putint(m);
    // do {
    //     forward(d);
    //     left(360 / x);
    //     m = m - 1;
    // } while (m > 0);
    /* これはコメント*
    これもコメント/
    ここまでコメント */
    while (m > 0) {
        forward(d);
        left(360/x);
        m = m - 1;
    }
    return 0;
}

fun int main(){
    int n;
    n = 40;
    draw(5);
    pen(1);
    forward(3 * n);
    draw(6);
    return 0;
}